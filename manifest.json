{
  "manifest_version": 2,
  "name": "FPV-KA Affiliation-Plugin",
  "version": "1.1",
  "homepage_url": "https://www.fpv-ka.de",
  "icons": {
    "48": "icons/fpv_small.jpg"
  },

  "content_scripts": [
    {
      "matches": ["*://*.banggood.com/*.html*"],
      "js": ["banggood-ref.js"]
    }
  ],
  
  "options_ui": {
    "page": "options.html"
  },
  
  "permissions": ["storage"],

  "applications": {
    "gecko": {
      "id": "info.fpv-ka@gmail.com"
    }
  }

}
